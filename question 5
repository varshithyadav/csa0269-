#include <stdio.h>

int main() {
    char c;

    printf("Enter a character: ");
    scanf("%c", &c);

    if(c >= 'A' && c <= 'Z') {
        printf("The entered character is an uppercase alphabet.\n");
    }
    else if(c >= 'a' && c <= 'z') {
        printf("The entered character is a lowercase alphabet.\n");
    }
    else {
        printf("The entered character is not an alphabet.\n");
    }

    return 0;
}
