#include <stdio.h>

int main() {
   int number, reverse = 0;

   printf("Enter a number: ");
   scanf("%d", &number);

   // loop to reverse the number
   for(; number != 0; number /= 10) {
      reverse = reverse * 10 + number % 10;
   }

   printf("The reverse of the given number is: %d\n", reverse);
   
   return 0;
}
